<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="by.bstu.fit.ramanovich.searchfortravelcompanions">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="by.bstu.fit.romanovich.searchfortravelcompanions.Activity.EditRequestActivity"></activity>
        <activity android:name="by.bstu.fit.romanovich.searchfortravelcompanions.Activity.AddRequestActivity" />
        <activity android:name="by.bstu.fit.romanovich.searchfortravelcompanions.Activity.RequestActivity" />
        <activity android:name="by.bstu.fit.romanovich.searchfortravelcompanions.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>